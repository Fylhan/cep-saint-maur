<?xml version="1.0" encoding="{{ Encodage }}" ?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{%  for url in UrlEntries %}
	<url>
	{% if "" != url.loc %}
		<loc>{{ url.loc }}</loc>
		{% if "" != url.lastmod %}<lastmod>{{ url.lastmod|date("Y-m-d") }}</lastmod>{% endif %}
		{% if "" != url.changefreq %}<changefreq>{{ url.changefreq }}</changefreq>{% endif %}
		{% if "" != url.priority %}<priority>{{ url.priority }}</priority>{% endif %}
	{% endif %}
	</url>
	{% if "" != url.children %}
		{% for urlChild in url.children %}
		<url>
			<loc>{{ urlChild.loc }}</loc>
			{% if "" != urlChild.lastmod %}<lastmod>{{ urlChild.lastmod|date("Y-m-d") }}</lastmod>{% endif %}
			{% if "" != urlChild.changefreq %}<changefreq>{{ urlChild.changefreq }}</changefreq>{% endif %}
			{% if "" != urlChild.priority %}<priority>{{ urlChild.priority }}</priority>{% endif %}
		</url>
		{% endfor %}
	{% endif %}
{% endfor %}
</urlset>
