{
	"name" : "fylhan/cep-saint-maur",
	"type" : "project",
	"description" : "Site Web du CEP Saint-Maur",
	"homepage" : "http://cepsaintmaur.fr",
	"license" : "LGPL-3.0",
	"authors" : [{
			"name" : "Olivier Maridat",
			"homepage" : "http://olivier.maridat.com",
			"role" : "developper"
		}
	],
	"config" : {
		"preferred-install" : "dist",
		"optimize-autoloader" : true,
		"discard-changes" : true
	},
	"require" : {
		"php" : ">=5.5",
		"twig/twig" : "1.*",
		"fguillot/picodb" : "1.0.7",
		"fguillot/simpleLogger" : "1.0.0",
		"pimple/pimple" : "~3.0",
		"swiftmailer/swiftmailer" : "~5.4",
		"symfony/console" : "~2.7",
		"symfony/event-dispatcher" : "~2.7"
	},
	"require-dev" : {
		"phpunit/phpunit" : "5.1.*"
	},
	"autoload" : {
		"psr-4" : {
			"" : "src/"
		},
		"files" : [
			"src/Utils/tools.php",
			"src/Library/FeedWriter/FeedWriter.php",
			"src/Library/FeedWriter/FeedItem.php",
			"src/Library/FeedWriter/FeedTypes.php"
		]
	},
	"support" : {
		"source" : "https://git.framasoft.org/fylhan/cep-saint-maur",
		"issues" : "https://trello.com/b/C2MTF6q8/cep-saint-maur"
	}
}